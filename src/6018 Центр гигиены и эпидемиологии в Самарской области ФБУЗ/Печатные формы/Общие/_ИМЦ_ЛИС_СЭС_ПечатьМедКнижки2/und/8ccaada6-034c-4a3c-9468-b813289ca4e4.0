&НаСервере
Процедура ПриСозданииНаСервере(Отказ, СтандартнаяОбработка)
	
	Если Параметры.Свойство(""СсылкаНаОбъект"") Тогда		
		СсылкаНаОбъект = Параметры.СсылкаНаОбъект;  		
	КонецЕсли;
	
КонецПроцедуры

&НаКлиенте
Процедура ПриОткрытии(Отказ)
	
	Если ЗначениеЗаполнено(СсылкаНаОбъект) Тогда
		
		ТабДок = Новый ТабличныйДокумент;
		ПечатьМедКнижкиНаСервере(ТабДок);
		//ТабДок.Показать();
		ИмяСпецПринтера = ИмяСпецПринтера();
		Если ЗначениеЗаполнено(ИмяСпецПринтера) Тогда
			ТабДок.ИмяПринтера = ИмяСпецПринтера;
		КонецЕсли;
		ТабДок.Напечатать();
		Отказ = Истина;
		
	КонецЕсли;
	
	
КонецПроцедуры  

